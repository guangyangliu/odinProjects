<html>
    <head>
        <link rel="stylesheet" href="/css/create.css">
        <link rel="stylesheet" href="/css/header.css">
        <link rel="stylesheet" href="/css/form.css">
    </head>
    <body>
        <div id="container">
            <%- include("header.ejs") %>
            <form method="post" action="/create/category">
                <label for="type">Car type: <input type="text" name="type" id="type" placeholder="SUV" required></label>
                <label for="name">Car name: <input type="text" name="name" id="name" placeholder="X3" required></label>
                
                <label for="img">Image url: <input type="text" name="img" id="img" required></label>
                <input type="submit" value="submit">
            </form>
        </div>
    </body>
</html>